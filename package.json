    "lint": "node_modules/.bin/esw source --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
