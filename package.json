    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.0",
